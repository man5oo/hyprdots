    subtext1: rgba(181,191,226,100%);

    surface0: rgba(65,69,89,100%);
    surface1: rgba(81,87,109,100%);
    surface2: rgba(98,104,128,100%);

    overlay0: rgba(115,121,148,100%);
    overlay1: rgba(131,139,167,100%);
